loop:
	show @a               # print(a)
	mov @a @c             # c = a
	add @b @c             # c += b
	mov @b @a             # a = b
	mov @c @b             # b = c
	add 001 @iter         # iter++
	jlt @iter 10 @loop   # if (iter < 0x10) goto loop
	halt                  # exit

a:
	0
b:
	1
c:
	0
iter:
	0
